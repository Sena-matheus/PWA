{
  "name": "Busca de Filmes com OMDb",
  "short_name": "Busca de Filmes",
  "description": "Aplicação para buscar filmes usando a API OMDb, desenvolvida com Express e JavaScript.",
  "start_url": "/moviefinderall.netlify.app",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#007bff",
  "icons": [
    {
      "src": "images/icon-192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "images/icon-512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "orientation": "portrait-primary",
  "scope": "/",
  "lang": "en",
  "serviceworker": {
    "src": "service-worker.js",
    "scope": "/",
    "type": "module"
  },
  "id": "movie",
  "dir": "auto",
  "display_override": [
    "fullscreen",
    "window-controls-overlay",
    "standalone"
  ],
  "prefer_related_applications": true,
  "categories": [
    "books",
    "entertainment",
    "games",
    "music"
  ],
  "edge_side_panel": {
    "preferred_width": 1
  }
}